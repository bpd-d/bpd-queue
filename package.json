{
	"name": "bpd-queue",
	"version": "1.0.0",
	"description": "Simple queue implementation which can be used everywhere function must be executed in a specific order",
	"main": "./dist/esm/index.js",
	"type": "module",
	"files": [
		"/dist/*"
	],
	"exports": {
		"import": "./dist/esm/index.js",
		"require": "./dist/main.cjs"
	},
	"scripts": {
		"build": "webpack --config webpack.prod.cjs",
		"build:minify": "webpack --config webpack.minify.cjs",
		"build:esm": "tsc -p tsconfig.esm.json",
		"test": "karma start karma.conf.cjs",
		"prod": "npm run build && npm run build:esm && npm run build:minify",
		"dev": "webpack serve --config webpack.dev.cjs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bpd-d/bpd-queue.git"
	},
	"keywords": [],
	"author": "BPDev",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/bpd-d/bpd-queue/issues"
	},
	"homepage": "https://github.com/bpd-d/bpd-queue#readme",
	"devDependencies": {
		"@types/jasmine": "^3.7.8",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"html-webpack-plugin": "^5.3.2",
		"jasmine-core": "^3.8.0",
		"karma": "^6.3.4",
		"karma-chrome-launcher": "^3.1.0",
		"karma-cli": "^2.0.0",
		"karma-jasmine": "^4.0.1",
		"karma-webpack": "^5.0.0",
		"terser-webpack-plugin": "^5.1.4",
		"webpack": "^5.43.0",
		"webpack-cli": "^4.7.2",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"fork-ts-checker-webpack-plugin": "^6.2.12",
		"karma-typescript": "^5.5.1",
		"ts-loader": "^9.2.3",
		"typescript": "^4.3.5"
	}
}
